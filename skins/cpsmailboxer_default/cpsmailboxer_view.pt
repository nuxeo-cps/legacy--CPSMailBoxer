    <div metal:fill-slot="header">
      <h2 i18n:translate="">nmb_heading_mailing_list &nbsp;<tal:block replace="here/title_or_id" i18n:name="title" />&nbsp;</h2>
      <i tal:content="here/Description|nothing" />
    </div>
    <div metal:fill-slot="main">	  
